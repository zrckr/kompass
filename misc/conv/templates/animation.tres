[gd_resource type="Animation" load_steps=2 format=3]

[ext_resource path="res://assets/sprites/${folder}/${name}.png" type="Texture" id=${id}]

[resource]
resource_name = ${name}
length = ${length}
tracks/0/type = "value"
tracks/0/path = NodePath("Sprite:region_rect")
tracks/0/interp = 1
tracks/0/loop_wrap = true
tracks/0/imported = false
tracks/0/enabled = true
tracks/0/keys = {
"times": PoolRealArray(${times}),
"transitions": PoolRealArray(${transitions}),
"update": 1,
"values": [${values}]
}
tracks/1/type = "value"
tracks/1/path = NodePath("Sprite:texture")
tracks/1/interp = 1
tracks/1/loop_wrap = true
tracks/1/imported = false
tracks/1/enabled = true
tracks/1/keys = {
"times": PoolRealArray(0),
"transitions": PoolRealArray(1),
"update": 1,
"values": [ExtResource(${id})]
}
tracks/2/type = "value"
tracks/2/path = NodePath("Sprite:offset")
tracks/2/interp = 1
tracks/2/loop_wrap = true
tracks/2/imported = false
tracks/2/enabled = true
tracks/2/keys = {
"times": PoolRealArray(0),
"transitions": PoolRealArray(1),
"update": 1,
"values": [${offset}]
}